<!doctype html>
<html class="no-js" lang="{{ request.locale.iso_code }}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta name="theme-color" content="">
    <link rel="canonical" href="{{ canonical_url }}">
    <link href="{{ 'themify-icons.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'font-awesome.min.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'flaticon_ecommerce.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'bootstrap.min.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'animate.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'owl.carousel.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'owl.theme.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'slick.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'slick-theme.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'swiper.min.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'owl.transitions.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'magnific-popup.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'jquery.fancybox.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'nice-select.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'odometer-theme-default.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'style.css' | asset_url }}" rel="stylesheet">
    <link
      rel="preconnect"
      href="https://cdn.shopify.com"
      crossorigin>
    <link
      rel="shortcut icon"
      type="image/png"
      href="{{ 'favicon.png' | asset_url }}">
    <title>
      {{ page_title }}
      {%- if current_tags %}
        &ndash; tagged "{{ current_tags | join: ', ' }}"{% endif -%}
      {%- if current_page != 1 %}
        &ndash; Page {{ current_page }}{% endif -%}
      {%- unless page_title contains shop.name %}
        &ndash; {{ shop.name }}{% endunless -%}
    </title>
    {% if page_description %}
      <meta name="description" content="{{ page_description | escape }}">
    {% endif %}

    {% render 'meta-tags' %}

    {{ content_for_header }}
  </head>

  <body>
    <div class="page-wrapper">
      <!-- start preloader -->
      <div class="preloader">
        <div class="vertical-centered-box">
          <div class="content">
            <div class="loader-circle"></div>
            <div class="loader-line-mask">
              <div class="loader-line"></div>
            </div>
            <img
              src="{{ 'preloader.png' | asset_url }}"
              alt=""
              width="100%"
              height="100%"
              loading="eager">
          </div>
        </div>
      </div>
      <!-- end preloader -->
      <div class="smooth-scrool-animate" id="has_smooth"></div>
      <div id="smooth-wrapper">
        <div id="smooth-content">

          {% section 'hearder' %}

          {{ content_for_layout }}

          {% section 'footer' %}

        </div>
      </div>
    </div>

    <script src="{{ 'jquery.min.js' | asset_url }}" defer></script>
    <script src="{{ 'bootstrap.bundle.min.js' | asset_url }}" defer></script>
    <!-- Plugins for this template -->
    <script src="{{ 'modernizr.custom.js' | asset_url }}" defer></script>
    <script src="{{ 'gsap.min.js' | asset_url }}" defer></script>
    <script src="{{ 'ScrollSmoother.min.js' | asset_url }}" defer></script>
    <script src="{{ 'ScrollTrigger.min.js' | asset_url }}" defer></script>
    <script src="{{ 'jquery-plugin-collection.js' | asset_url }}" defer></script>
    <!-- Custom script for this template -->
    <script src="{{ 'gsap-active.js' | asset_url }}" defer></script>
    <script src="{{ 'script.js' | asset_url }}" defer></script>
  </body>
</html>